
#include <stdio.h>

int main() {
    int marks;
    printf("enter marks");
    scanf("%d",&marks);
    if (marks>=90)
    printf(" grade A\n");
    else if (marks>=75)
    printf("grade B\n");
    else if (marks>=60)
    printf("grade C\n");
    else if (marks>=35)
    printf("grade D\n");
    else 

    printf("fail\n");

    return 0;
}
